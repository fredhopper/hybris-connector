					<xsd:enumeration value="itemnotfounduniverse" />

					<xsd:enumeration value="emptykeeplist" />

					<xsd:enumeration value="missing-arguments" />

					<xsd:enumeration value="search-noresults" />

					<xsd:enumeration value="loading" />

				</xsd:restriction>

			</xsd:simpleType>


			<xsd:simpleType name="yesNo">

				<xsd:restriction base="xsd:string">

					<xsd:enumeration value="yes" />

