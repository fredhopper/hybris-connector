					<xsd:enumeration value="s" />

				</xsd:restriction>

			</xsd:simpleType>


			<xsd:simpleType name="viewType">

				<xsd:restriction base="xsd:string">

					<xsd:enumeration value="index" />

					<xsd:enumeration value="lister" />

					<xsd:enumeration value="detail" />

					<xsd:enumeration value="search" />

					<xsd:enumeration value="keeplist" />

					<xsd:enumeration value="compare" />

					<xsd:enumeration value="home" />

					<xsd:enumeration value="summary" />

					<xsd:enumeration value="redirect" />

					<xsd:enumeration value="empty" />

				</xsd:restriction>

			</xsd:simpleType>


			<xsd:simpleType name="attributeTypeFormat">

				<xsd:restriction base="xsd:string">

					<xsd:enumeration value="int" />

					<xsd:enumeration value="float" />

					<xsd:enumeration value="text" />

					<xsd:enumeration value="list" />

					<xsd:enumeration value="set" />

					<xsd:enumeration value="date" />

					<xsd:enumeration value="ref" />

					<xsd:enumeration value="intset" />

					<xsd:enumeration value="cat" />

					<xsd:enumeration value="asset" />

